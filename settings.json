{
    "editor.accessibilitySupport": "off",
    "editor.cursorSurroundingLines": 10,
    "editor.fontFamily": "'Iosevka SS04', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.formatOnSave": true,
    "editor.minimap.enabled": false,
    "workbench.enableExperiments": false,
    "workbench.iconTheme": "vs-minimal",
    "workbench.tips.enabled": false,
    "editor.fontSize": 15,
    "rust-client.engine": "rust-analyzer",
    "editor.quickSuggestions": false,
    "editor.suggestOnTriggerCharacters": false,
    "workbench.colorTheme": "Syntax-almost-off",
    "editor.cursorStyle": "block",
    "editor.renderLineHighlight": "all",
    "editor.renderLineHighlightOnlyWhenFocus": true,
    "window.restoreWindows": "none",
    "editor.tabSize": 2,
    "editor.wordWrapColumn": 99,
    "workbench.settings.enableNaturalLanguageSearch": false,
    "npm.fetchOnlinePackageInfo": false,
    "editor.codeLens": false,
    "editor.insertSpaces": false,
    "vim.statusBarColorControl": true,
    "vim.statusBarColors.normal": "#333333",
    "vim.statusBarColors.insert": "#121212",
    "vim.statusBarColors.visual": "#333333",
    "vim.statusBarColors.visualline": "#333333",
    "vim.statusBarColors.visualblock": "#333333",
    "vim.statusBarColors.replace": "#121212",
    "vim.statusBarColors.commandlineinprogress": "#121212",
    "vim.statusBarColors.searchinprogressmode": "#333333",
    "vim.statusBarColors.easymotionmode": "#333333",
    "vim.statusBarColors.easymotioninputmode": "#333333",
    "vim.statusBarColors.surroundinputmode": "#121212",
    "workbench.colorCustomizations": {
        "statusBar.background": "#333333",
        "statusBar.noFolderBackground": "#333333",
        "statusBar.debuggingBackground": "#333333"
    }
}