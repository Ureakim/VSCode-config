{
	"editor.accessibilitySupport": "off",
	"editor.cursorSurroundingLines": 10,
	"editor.fontFamily": "'Hack', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
	"editor.formatOnSave": true,
	"editor.minimap.enabled": false,
	"workbench.enableExperiments": false,
	"workbench.tips.enabled": false,
	"editor.quickSuggestions": {
		"comments": "off",
		"strings": "off",
		"other": "off"
	},
	"editor.suggestOnTriggerCharacters": false,
	"workbench.colorTheme": "Nebulae",
	"editor.renderLineHighlight": "all",
	"editor.renderLineHighlightOnlyWhenFocus": true,
	"window.restoreWindows": "none",
	"editor.tabSize": 2,
	"editor.wordWrapColumn": 99,
	"workbench.settings.enableNaturalLanguageSearch": false,
	"npm.fetchOnlinePackageInfo": false,
	"editor.codeLens": false,
	"editor.insertSpaces": false,
	"vim.statusBarColorControl": true,
	"vim.statusBarColors.normal": "#333333",
	"vim.statusBarColors.insert": "#121212",
	"vim.statusBarColors.visual": "#333333",
	"vim.statusBarColors.visualline": "#333333",
	"vim.statusBarColors.visualblock": "#333333",
	"vim.statusBarColors.replace": "#121212",
	"vim.statusBarColors.commandlineinprogress": "#121212",
	"vim.statusBarColors.searchinprogressmode": "#333333",
	"vim.statusBarColors.easymotionmode": "#333333",
	"vim.statusBarColors.easymotioninputmode": "#333333",
	"vim.statusBarColors.surroundinputmode": "#121212",
	"[rust]": {
		"editor.defaultFormatter": "matklad.rust-analyzer",
	},
	"workbench.colorCustomizations": {
		"statusBar.background": "#333333",
		"statusBar.noFolderBackground": "#333333",
		"statusBar.debuggingBackground": "#333333"
	},
	"editor.cursorStyle": "block-outline",
	"vim.autoindent": false,
	"editor.lineNumbers": "relative",
	"files.enableTrash": false,
	"workbench.startupEditor": "newUntitledFile",
	"diffEditor.ignoreTrimWhitespace": true,
	"files.insertFinalNewline": true,
	"files.trimFinalNewlines": true,
	"workbench.tree.renderIndentGuides": "always",
	"files.watcherExclude": {
		"**/target/**": true
	},
	"rust-analyzer.inlayHints.enable": false,
	"editor.fontLigatures": true,
	"workbench.editor.untitled.hint": "hidden",
	"telemetry.enableCrashReporter": false,
	"telemetry.enableTelemetry": false,
	"http.proxySupport": "off",
	"liveshare.presence": false,
	"workbench.tree.indent": 15,
	"editor.stickyScroll.enabled": true,
	"telemetry.telemetryLevel": "off",
	"editor.inlayHints.enabled": "offUnlessPressed",
}
